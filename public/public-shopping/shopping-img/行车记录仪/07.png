            if ("" === text) {
                $(tar).val("1");
                $(tar).trigger("setvalue");
                return;
            }

            if (!/^\d+$/.test(text)) {
                $(tar).val("1");
                $(tar).trigger("setvalue");
                return;
            }

            var val = parseInt(text, 10);
            var max = '0' | 0;
            var storeNum = parseInt('74', 10);

            if (val === 0) {
                $(tar).val("1");
                $(tar).trigger("setvalue");
                return;
            }

            if (max > 0 && val > max) {
                qikoo.dialog.alert("购买数量超出限制!");
                $(tar).val(max);

                $(tar).trigger("setvalue");
                return;
            }

            if (max == 0 && val > storeNum) {
                qikoo.dialog.alert("购买数量超出库存!");
                $(tar).val(storeNum);

                $(tar).trigger("setvalue");
                return;
            }

            $(tar).trigger("setvalue");
        });

